{
  "name": "microscholar-backend",
  "version": "1.0.0",
  "description": "Backend API for MicroScholar - Web3 Scholarship Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "compile": "npx hardhat compile",
    "deploy:local": "npx hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "npx hardhat run scripts/deploy.js --network sepolia",
    "deploy:v2:local": "npx hardhat run scripts/deployV2.js --network localhost",
    "deploy:v2:sepolia": "npx hardhat run scripts/deployV2.js --network sepolia",
    "deploy:v3:local": "npx hardhat run scripts/deployV3.js --network localhost",
    "deploy:v3:sepolia": "npx hardhat run scripts/deployV3.js --network sepolia",
    "node": "npx hardhat node",
    "test": "npx hardhat test",
    "test:v2": "npx hardhat test test/MicroScholarV2.test.js"
  },
  "keywords": [
    "web3",
    "scholarship",
    "blockchain",
    "express",
    "mongodb",
    "microscholar"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@supabase/supabase-js": "^2.77.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "groq-sdk": "^0.3.3",
    "joi": "^17.13.3",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "tesseract.js": "^5.0.4",
    "twilio": "^5.2.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "hardhat": "^2.19.0",
    "nodemon": "^3.0.2"
  }
}
